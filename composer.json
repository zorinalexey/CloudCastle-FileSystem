        "php": ">=7.4",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-spl": "*",
        "phpmd/phpmd": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
